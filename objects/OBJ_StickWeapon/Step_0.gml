	if  (keyboard_check_pressed(ord("D")))
	{
		image_angle = 0;
		image_xscale = 1;
		image_yscale = 1;
	}
	if  (keyboard_check_pressed(ord("A")))
	{
		image_angle = 180;
		image_yscale = -1;
	}
	if  (keyboard_check_pressed(ord("W")))
	{
		image_angle = 90;
		image_xscale = 1
		image_yscale = 1;
	}
	if  (keyboard_check_pressed(ord("S")))
	{
		image_angle = 270;
		image_xscale = 1
		image_yscale = 1;
	}































































