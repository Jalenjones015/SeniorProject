	if  (keyboard_check_pressed(ord("D")))
	{
		image_angle = 0;
	}
	if  (keyboard_check_pressed(ord("A")))
	{
		image_angle = 180;
	}
	if  (keyboard_check_pressed(ord("W")))
	{
		image_angle = 90;
	}
	if  (keyboard_check_pressed(ord("S")))
	{
		image_angle = 270;
	}































































